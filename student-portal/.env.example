# Student Portal Environment Variables

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_APP_NAME=University Student Portal
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key
VITE_SESSION_TIMEOUT=3600000

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx

# Remita Payment Configuration
VITE_REMITA_MERCHANT_ID=your-remita-merchant-id
VITE_REMITA_SERVICE_TYPE_ID=your-service-type-id
VITE_REMITA_API_KEY=your-remita-api-key
VITE_REMITA_DEMO_MODE=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_PWA=false
