{
  "name": "university-record-management-system",
  "version": "1.0.0",
  "description": "A comprehensive university record management system built with MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "seed": "node utils/seedDatabase.js",
    "init-rbac": "node scripts/initializeRBAC.js",
    "init-notifications": "node scripts/initializeNotifications.js",
    "init-all": "npm run init-rbac && npm run init-notifications"
  },
  "keywords": [
    "university",
    "record-management",
    "student-portal",
    "admin-portal",
    "education",
    "mern-stack",
    "nodejs",
    "express",
    "mongodb",
    "react"
  ],
  "author": "University Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "aws-sdk": "^2.1691.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "mongoose": "^8.16.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
